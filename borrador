#include <stdio.h>
#include <stbool.h>


bool esPrimo(int n) {
    int tope = n * (n * 1/2);

    if (n <= 1) {
        return "False";
    } else {
        
        for (int i=2; i<=tope; i++) {
            if (n % i == 0) {
                return "False";
            }
            
        return "True";
        }

    }

    
}


int main() {

    int num1 = 0;
    int num2 = 1;
    int resultado = 0;
    int n;
    printf("Ingrese ")
    scanf("%d", &n);

    for (int i = 0; i <=n; i++) {
        resultado = num1 + num2;
        num1 = num2;
        num2 = resultado;

        if (esPrimo(resultado)) {
            printf("%d\n",resultado)
        }

    }


    return 0;
}

