#include <stdio.h>
#include <stdbool.h>
#include <math.h>


bool esPrimo(int n) {
    int tope = sqrt(n);

    if (n == 2) {
        return true;
    } else if (n <= 1) {
        return false;
    } else {
        
        for (int i=2; i<=tope; i++) {
            if (n % i == 0) {
                return false;
            }
        }
        return true;
    }

    
}


int main() {

    int num1, resultado = 0;
    int num2 = 1;
    int n;
    printf("Ingrese num: ");
    scanf("%d", &n);

    for (int i = 0; i <n; i++) {
        resultado = num1 + num2;
        num1 = num2;
        num2 = resultado;

        if (esPrimo(resultado)) {
            printf("%d\n",resultado);
        }

    }


    return 0;
}

